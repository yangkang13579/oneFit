/*顶部*/
.bg {
  height: 100%;
  width: 100%;
  background-image: linear-gradient(#23a7a2, #0d7fc6);
  position: fixed;
  top: 0;
  left: 0;
}
.top {
  border-radius: 10rpx;
  width: 690rpx;
  background: #a4ccdd;
  background: #f0f8ff;
  margin-top: 40rpx;
  margin-left: 28rpx;
}
.top > view {
  border-top: 1px solid #a4ccdd;
  padding: 0 30rpx 0 30rpx;
  display: flex;
  align-items: center;
  height: 96rpx;
  justify-content: space-between;
}
.top > view view {
  font-size: 30rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}
.top > view view image {
  width: 40rpx;
  height: 40rpx;
  margin-left: 24rpx;
  margin-right: 20rpx;
}
.top > view .img {
  width: 18rpx;
  height: 36rpx;
  margin-right: 10rpx;
}
.top view:nth-of-type(1) {
  border-top: 0;
}
.wrap {
  width: 690rpx;
  border-radius: 20rpx;
  margin: 40rpx 30rpx 0 30rpx;
  background: #fff;
}
.middle {
  width: 622rpx;
  height: 96rpx;
  font-size: 24rpx;
  border-top: 1px solid #e3e3e3;
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 0 34rpx;
}
.middle text {
  font-size: 32rpx;
}
.middle:nth-of-type(1) {
  border-top: 0;
}
.empty {
  position: fixed;
  top: 30%;
  left: 0;
  font-size: 32rpx;
  text-align: center;
  width: 100%;
  color: #fff;
}
.empty view {
  margin-top: 10rpx;
}
.toast {
  display: flex;
  align-items: center;
  position: fixed;
  top: 40%;
  justify-content: center;
  width: 100%;
}
.toast p {
  border-radius: 10rpx;
  background: rgba(0, 0, 0, 0.6);
  color: #fff;
  font-size: 30rpx;
  padding: 20rpx 30rpx;
}
.page-top {
  top: 0;
  left: 0;
  z-index: 2;
  position: relative;
}
.table {
  width: 690rpx;
  margin-left: 30rpx;
}
.table .tab-list {
  margin-top: 24rpx;
  border-radius: 0rpx 0rpx 20rpx 20rpx;
}
.table .tab-list .tab-title {
  width: 630rpx;
  padding: 0 20rpx 0 40rpx;
  border-radius: 20rpx 20rpx 0rpx 0rpx;
  height: 90rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  background: #e3e3e3;
}
.table .tab-list .tab-title view {
  font-size: 32rpx;
  color: #333333;
}
.table .tab-list .tab-title text {
  font-size: 24rpx;
  color: #333333;
}
.table .tab-list .tab-cont {
  width: 630rpx;
  padding: 10rpx 20rpx 0rpx 40rpx;
  background: #fff;
}
.table .tab-list .tab-cont view {
  width: 100%;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  font-size: 28rpx;
  height: 50rpx;
}
.table .tab-list .tab-cont view text {
  font-size: 28rpx;
  color: #0d7fc6;
  margin-left: 10rpx;
}
.table .tab-list .tab-img {
  width: 660rpx;
  background: #fff;
  padding: 10rpx 15rpx;
  border-radius: 0 0 20rpx 20rpx;
  clear: both;
  overflow: hidden;
}
.table .tab-list .tab-img .img-list {
  width: 100%;
  height: 230rpx;
  margin-bottom: 10rpx;
  display: flex;
  justify-content: flex-start;
  align-items: center;
}
.table .tab-list .tab-img .img-list .img-left {
  width: 330rpx;
  height: 230rpx;
}
.table .tab-list .tab-img .img-list .img-left image {
  display: block;
  width: 100%;
  height: 180rpx;
}
.table .tab-list .tab-img .img-list .img-left .new {
  width: 100%;
  height: 50rpx;
  font-size: 28rpx;
  color: #fff;
  font-weight: bolder;
  display: flex;
  justify-content: center;
  align-items: center;
  border-radius: 0 0 0 40rpx;
  background: #589a34;
}
.table .tab-list .tab-img .img-list .img-left .new1 {
  width: 100%;
  height: 50rpx;
  font-size: 28rpx;
  color: #fff;
  font-weight: bolder;
  display: flex;
  justify-content: center;
  align-items: center;
  background: #589a34;
}
.table .tab-list .tab-img .img-list .img-left .old {
  width: 100%;
  height: 50rpx;
  display: flex;
  font-size: 28rpx;
  color: #fff;
  font-weight: bolder;
  justify-content: center;
  align-items: center;
  border-radius: 0 0 40rpx 0rpx;
  background: #589a34;
}
.add {
  width: 671rpx;
  margin-left: 28rpx;
  background: #f0f8ff;
  padding: 30rpx 0 26rpx 20rpx;
}
.input-wrap {
  font-size: 30rpx;
  margin-bottom: 20rpx;
  padding-right: 20rpx;
  width: 630rpx;
}
.input-wrap p {
  color: #252525;
}
.input-wrap input,
.input-wrap picker {
  margin-top: 20rpx;
  padding: 0 20rpx;
  border: 1rpx solid #ccc;
  line-height: 70rpx;
  font-size: 30rpx;
  color: #333;
  border-radius: 10rpx;
  height: 70rpx;
  background: #fff;
  width: 610rpx;
}
.input-wrap textarea {
  margin-top: 20rpx;
  width: 650rpx;
  padding: 0 20rpx;
  line-height: 70rpx;
  font-size: 30rpx;
  color: #333;
  border-radius: 10rpx;
  height: 200rpx;
  background: #fff;
}
.input-wrap .smallview {
  display: flex;
  align-items: center;
}
.input-wrap .small {
  width: 320rpx;
}
.input-wrap .small input,
.input-wrap .small picker {
  width: 290rpx;
}
.input-wrap .small:nth-of-type(2) {
  margin-left: 30rpx;
}
.input-wrap:last-child {
  border: none;
}
.input-wrap text {
  height: 90rpx;
  line-height: 90rpx;
  color: #666;
  padding-left: 20rpx;
}
.code {
  border-top: 1rpx dashed #0558ac;
  padding-top: 12px;
}
.code .imgBox {
  width: 100%;
  height: 130rpx;
  display: flex;
  justify-content: flex-start;
  align-items: center;
}
.code .imgBox image {
  width: 92rpx;
  margin-top: 10rpx;
  height: 92rpx;
}
.weui-uploader__input-box {
  width: 92rpx;
  margin-top: 10rpx;
  height: 92rpx;
}
.save-btn {
  font-size: 30rpx;
  margin-left: 3rpx;
  margin-top: 26rpx;
  width: 48%;
  height: 90rpx;
  line-height: 90rpx;
  background: #0970AC;
  color: #fff;
  margin-bottom: 26rpx;
  float: left;
}
.del {
  background: #C4C4C4;
  float: right;
}
.isOpen {
  transform: rotate(90deg);
}
