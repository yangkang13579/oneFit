.com {
  width: 710rpx;
  padding: 0 20rpx;
}
.com .title {
  font-size: 32rpx;
  height: 90rpx;
  line-height: 90rpx;
}
.com .new {
  width: 100%;
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
  padding: 10rpx 0;
}
.com .new .new-item {
  width: 330rpx;
  height: 210rpx;
  margin-bottom: 30rpx;
}
.com .new .new-item image {
  width: 330rpx;
  height: 210rpx;
}
