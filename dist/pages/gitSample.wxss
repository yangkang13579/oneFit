page {
  background: #fff;
}
.myTitle {
  padding: 10rpx 20rpx;
  height: 140rpx;
  width: 710rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.myTitle .myName {
  width: 500rpx;
  height: 100rpx;
  display: flex;
  justify-content: flex-start;
  align-items: center;
}
.myTitle .myName image {
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
  margin-right: 30rpx;
}
.myTitle .myName text {
  font-size: 30rpx;
  color: #000;
}
.myTitle .myApply {
  width: 190rpx;
  height: 100rpx;
  display: flex;
  justify-content: flex-end;
  align-items: center;
}
.myTitle .myApply text {
  font-size: 30rpx;
  color: #000;
  margin-right: 20rpx;
}
.myTitle .myApply image {
  width: 26rpx;
  height: 26rpx;
}
.myList {
  width: 710rpx;
  padding: 20rpx;
  background: #f4f4f4;
}
.myList .myItem {
  width: 660rpx;
  padding: 30rpx 25rpx;
  border-radius: 20rpx;
  font-size: .1rem;
  background: #fff;
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-bottom: 20rpx;
}
.myList .myItem image {
  width: 200rpx;
  height: 220rpx;
}
.myList .myItem .myItem-right {
  width: 420rpx;
  height: 220rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}
.myList .myItem .myItem-right .myItem-title {
  width: 100%;
  font-size: 30rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 3;
  -webkit-box-orient: vertical;
}
.myList .myItem .myItem-right .myItem-bot {
  width: 100%;
  height: 90rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.myList .myItem .myItem-right .myItem-bot .btnLeft {
  font-size: 30rpx;
  color: #5e99a6;
}
.myList .myItem .myItem-right .myItem-bot .btnSample {
  width: 200rpx;
  height: 60rpx;
  text-align: center;
  line-height: 60rpx;
  border-radius: 30rpx;
  font-size: 30rpx;
  background: #dbe6e9;
  color: #2b779a;
}
