@import "./../../components/headers.wxss";
page {
  background: #111111;
}
.headers {
  height: 160rpx;
  width: 700rpx;
  margin-left: 25rpx;
  background: #2d2d2d;
  border-radius: 10rpx;
  display: flex;
  align-items: center;
}
.headers image {
  width: 142rpx;
  height: 142rpx;
  border-radius: 50%;
  margin: 0 38rpx 0 22rpx;
}
.headers .headers-top {
  width: 350rpx;
  text-align: center;
}
.headers .headers-top .tit {
  font-size: 36rpx;
  color: #e02510;
  font-weight: bold;
}
.headers .headers-top view {
  font-size: 26rpx;
  color: #acacac;
}
