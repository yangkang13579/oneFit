page {
  background: #f4f4f4;
  width: 100%;
  height: 100%;
}
.back {
  position: fixed;
  top: 50rpx;
  left: 20rpx;
  width: 35rpx;
  height: 35rpx;
  z-index: 5;
}
.top {
  width: 100%;
  height: 400rpx;
}
.top .banner {
  width: 100%;
  height: 400rpx;
}
.con {
  width: 730rpx;
  height: auto;
  background: #fff;
  margin-left: 10rpx;
}
.con .title {
  width: 690rpx;
  padding: 30rpx 20rpx;
  border-bottom: 2rpx solid #f4f4f4;
  display: flex;
  justify-content: space-between;
}
.con .title .title-left {
  width: 550rpx;
}
.con .title .title-left view {
  width: 550rpx;
  font-size: 32rpx;
  color: #000;
  margin-bottom: 20rpx;
}
.con .title .title-left text {
  font-size: 26rpx;
  color: #b6b6b6;
}
.con .title .title-right {
  display: flex;
  height: 40rpx;
  justify-content: flex-start;
  align-items: flex-end;
}
.con .title .title-right .one {
  font-size: 24rpx;
  color: #e5464a;
}
.con .title .title-right .two {
  font-size: 28rpx;
  font-weight: bolder;
  color: #e5464a;
}
.con .title .title-right .three {
  font-size: 24rpx;
  color: #e5464a;
}
.con .name {
  width: 690rpx;
  position: relative;
  padding: 30rpx 20rpx;
  border-bottom: 2rpx solid #f4f4f4;
  display: flex;
  justify-content: flex-start;
  align-items: center;
}
.con .name .name-img {
  width: 120rpx;
  height: 120rpx;
  border-radius: 50%;
}
.con .name .name-text {
  width: 550rpx;
  margin-left: 20rpx;
}
.con .name .name-text .name-title {
  font-size: 32rpx;
  color: #000;
  margin-bottom: 30rpx;
}
.con .name .name-text .name-list {
  width: 100%;
  font-size: 26rpx;
  color: #b6b6b6;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.con .name .name-text .name-detail {
  width: 100%;
  font-size: 26rpx;
  color: #b6b6b6;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.con .name .position {
  position: absolute;
  width: 30rpx;
  height: 30rpx;
  right: 5rpx;
  top: 85rpx;
}
.con .lei {
  width: 690rpx;
  padding: 30rpx 20rpx;
  border-bottom: 2rpx solid #f4f4f4;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}
.con .lei .time,
.con .lei .adress,
.con .lei .tel {
  display: flex;
  height: 60rpx;
  justify-content: flex-start;
  align-items: center;
}
.con .lei .time image,
.con .lei .adress image,
.con .lei .tel image {
  width: 35rpx;
  height: 35rpx;
  margin-right: 30rpx;
}
.con .lei .time text,
.con .lei .adress text,
.con .lei .tel text {
  color: #7c7c7c;
  font-size: 26rpx;
}
.con .people {
  width: 690rpx;
  padding: 30rpx 20rpx;
  border-bottom: 2rpx solid #f4f4f4;
}
.con .people .people-title {
  display: flex;
  justify-content: flex-start;
  margin-bottom: 30rpx;
}
.con .people .people-title image {
  width: 35rpx;
  height: 35rpx;
  margin-right: 30rpx;
}
.con .people .people-title text {
  color: #7c7c7c;
  font-size: 28rpx;
}
.con .people .people-list {
  width: 100%;
  display: flex;
  justify-content: flex-start;
  align-items: center;
}
.con .people .people-list image {
  width: 70rpx;
  height: 70rpx;
  border-radius: 50%;
  margin-right: 20rpx;
}
.bottom {
  width: 710rpx;
  padding: 0 20rpx;
  height: 120rpx;
  background: #fff;
  position: fixed;
  left: 0;
  bottom: 0;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.bottom .btnres {
  width: 340rpx;
  height: 90rpx;
  background: #e5464a;
  text-align: center;
  line-height: 90rpx;
  font-size: 30rpx;
  font-weight: bolder;
  color: #fff;
  border-radius: 45rpx;
}
.bottom .btnbuy {
  width: 340rpx;
  height: 90rpx;
  background: #e5464a;
  text-align: center;
  line-height: 90rpx;
  font-size: 30rpx;
  font-weight: bolder;
  color: #fff;
  border-radius: 45rpx;
}
