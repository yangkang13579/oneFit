button::after {
  border: none;
}
button {
  background-color: transparent;
  padding-left: 0;
  padding-right: 0;
  line-height: inherit;
}
button {
  border-radius: 0;
}
.banner {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
}
.header {
  font-size: 30rpx;
  color: #fff;
  width: 100%;
  position: fixed;
  z-index: 1;
}
.header text {
  position: absolute;
  bottom: 5rpx;
  left: 15rpx;
}
.center {
  position: fixed;
  top: 23%;
  z-index: 2;
  width: 100%;
}
.center .icon2 {
  width: 444rpx;
  height: 444rpx;
  position: absolute;
  margin-left: 143rpx;
}
.center .icon3 {
  width: 320rpx;
  height: 320rpx;
  position: absolute;
  margin-left: 204rpx;
  margin-top: 62rpx;
}
.btn {
  position: fixed;
  top: 61%;
  width: 500rpx;
  margin-left: 125rpx;
}
.btn .btnUser {
  width: 500rpx;
  height: 70rpx;
  background: #e02510;
  color: #fff;
  font-size: 28rpx;
  text-align: center;
  line-height: 70rpx;
  border-radius: 45rpx;
  padding: 0;
  border: 0;
}
.btn .btnBack {
  margin-top: 30rpx;
  width: 500rpx;
  height: 70rpx;
  border: 1rpx solid #e02510;
  color: #fff;
  background: 0;
  font-size: 28rpx;
  text-align: center;
  line-height: 70rpx;
  border-radius: 45rpx;
  padding: 0;
}
