page {
  background: #111111;
}
.header {
  font-size: 30rpx;
  color: #fff;
  width: 100%;
  position: fixed;
  background: #111111;
  z-index: 1;
}
.header text {
  position: absolute;
  left: 15rpx;
}
.name-header {
  width: 700rpx;
  margin-left: 25rpx;
  height: 401rpx;
  position: relative;
}
.name-header .icon42 {
  width: 700rpx;
  height: 401rpx;
  position: absolute;
  left: 0;
  top: 0;
}
.name-header .name {
  position: absolute;
  top: 37rpx;
  left: 35rpx;
  font-size: 30rpx;
  color: #fff;
  display: flex;
  align-items: center;
}
.name-header .name image {
  width: 144rpx;
  height: 144rpx;
  border-radius: 50%;
  border: 2rpx solid #fff;
  margin-right: 35rpx;
}
.list {
  margin-top: 54rpx;
}
.list .items {
  width: 100%;
  padding: 25rpx 0;
  font-size: 30rpx;
  font-weight: bold;
  position: relative;
}
.list .items .icon26 {
  position: absolute;
  bottom: 0;
  width: 100%;
  height: 4rpx;
  left: 0;
}
.list .items view {
  margin: 0 64rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  color: #fff;
  font-weight: bold;
}
.list .items view image {
  width: 42rpx;
  height: 40rpx;
}
