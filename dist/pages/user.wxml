
  <view class="container flex">
    <!-- 头像和昵称-->
    <view class="my">
      <view class="myHead"/>
      <!--<view class="myId">
            用户ID : {{user.id}}
          </view>-->
      <view class="myIcon">
        <image src="{{user.profile}}"/>
      </view>
      <view class="myName">
        {{user.nickName}}
      </view>
      <view class="myfost">
        <view bindtap="call" class="mykf">
          <image src="https://cdn.sunjoypai.com/images/0874/2476/2174/27bb36bebc79fdee6513c095120e9581_0_0.png"/>
          <text>客服</text>
        </view>
        <view bindtap="set" class="mykf">
          <image src="https://cdn.sunjoypai.com/images/9029/1241/0723/943e858f63b17d8f94d44f60ed547378_0_0.png"/>
          <text>设置</text>
        </view>
      </view>
     
      <view class="mine-content">
        <view class="tab-item line" catchtap="toMyCommunity">
          <view class="tab-item-left">
            <image style="float:left;width:37rpx;height:37rpx;margin-right:15rpx;" src="https://cdn.sunjoypai.com/images/9017/7438/8763/4c31696b89e0992e62dfe7ee21ec4fc4_0_0.png"/>
            <text>我的订单</text>
          </view>
          <view class="arrow"/>
        </view>
      </view>
    </view>
  </view>
