@import "./../../components/headers.wxss";
page {
  background: #111111;
}
.items {
  margin: 165rpx 25rpx 0 25rpx;
  width: 700rpx;
  min-height: 908rpx;
  background: #2d2d2d;
  clear: both;
  overflow: hidden;
}
.items .item-list {
  margin-left: 20rpx;
  margin-top: 20rpx;
  margin-bottom: 15rpx;
  float: left;
}
.items .item-list image {
  width: 320rpx;
  height: 240rpx;
}
.items .item-list view {
  font-size: 20rpx;
  color: #acacac;
}
.items .item-list view text {
  margin-left: 15rpx;
}
