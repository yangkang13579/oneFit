

  <view>
    <view class="bg"/>
    <view class="page-top">
    <view class="top" bindtap="createFun">
      <view>
      <image src="../images/ic_test5.png"/>
      <text>创建实验</text>
      </view>
      <image class="img" src="../images/arrow_r@3x.png"/>
    </view>
    <view class="wrap"> 
      <block  wx:for="{{list}}" wx:key="id" wx:for-index="idx">
        <view class="middle" bindtap="go" data-id="{{item.id}}">
          <view>
            {{item.name}}
          </view>
          {{item.createDate}}
        </view>
      </block>
    </view>
    <view class="toast"  wx:if="{{showToast}}"><p>{{error}}</p></view>
  </view>
  </view>
