page {
  background: #f4f4f4;
  width: 100%;
  height: 100%;
}
.order {
  width: 710rpx;
  padding: 30rpx 20rpx;
  background: #fff;
}
.order .order-top {
  width: 100%;
  display: flex;
  justify-content: space-between;
  padding-bottom: 30rpx;
  border-bottom: 2rpx solid #f4f4f4;
}
.order .order-top image {
  width: 250rpx;
  height: 140rpx;
  margin-right: 20rpx;
}
.order .order-top .order-title {
  width: 450rpx;
  height: 140rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
}
.order .order-top .order-title .name {
  font-size: 30rpx;
  font-weight: bolder;
}
.order .order-top .order-title .price {
  display: flex;
  justify-content: flex-start;
  align-items: center;
}
.order .order-top .order-title .price .one {
  font-size: 24rpx;
  color: #e5464a;
}
.order .order-top .order-title .price .two {
  font-size: 28rpx;
  font-weight: bolder;
  color: #e5464a;
}
.order .order-top .order-title .price .three {
  font-size: 24rpx;
  color: #e5464a;
}
.order .lei {
  width: 100%;
  padding: 20rpx 0;
}
.order .lei .time,
.order .lei .adress,
.order .lei .tel,
.order .lei .coach {
  display: flex;
  height: 60rpx;
  justify-content: flex-start;
  align-items: center;
}
.order .lei .time image,
.order .lei .adress image,
.order .lei .tel image,
.order .lei .coach image {
  width: 35rpx;
  height: 35rpx;
  margin-right: 30rpx;
}
.order .lei .time text,
.order .lei .adress text,
.order .lei .tel text,
.order .lei .coach text {
  color: #323232;
  font-size: 26rpx;
}
.number {
  width: 710rpx;
  padding: 0rpx 20rpx;
  background: #fff;
}
.number .num-top {
  padding: 30rpx 0rpx;
  border-bottom: 2rpx solid #f4f4f4;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.number .num-top .numb {
  width: 200rpx;
  font-size: 30rpx;
  color: #000;
}
.number .num-top .box {
  display: flex;
  width: 400rpx;
  height: 70rpx;
  overflow-x: auto;
}
.number .num-top .box .box-item {
  flex-shrink: 0;
}
.number .num-top .box .list {
  float: left;
  width: 120rpx;
  height: 60rpx;
  text-align: center;
  line-height: 60rpx;
  border-radius: 10rpx;
  font-size: 26rpx;
  color: #000;
  background: #f4f4f4;
  margin-right: 20rpx;
}
.number .num-top .box .active {
  background: #e5464a;
  color: #fff;
}
.number .pay {
  padding: 30rpx 0rpx;
}
.number .pay .pay-title {
  font-size: 28rpx;
  margin-bottom: 50rpx;
}
.number .pay .pay-list {
  width: 100%;
}
.number .pay .pay-list .pay-top {
  width: 100%;
  height: 90rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.number .pay .pay-list .pay-top .top-left {
  width: 300rpx;
  height: 90rpx;
  display: flex;
  justify-content: flex-start;
  align-items: center;
}
.number .pay .pay-list .pay-top .top-left image {
  width: 45rpx;
  height: 35rpx;
}
.number .pay .pay-list .pay-top .top-left text {
  font-size: 28rpx;
  font-weight: bolder;
  color: #000;
  margin-left: 20rpx;
}
.number .pay .pay-list .pay-top .top-right {
  width: 300rpx;
  height: 90rpx;
  display: flex;
  justify-content: flex-end;
  align-items: center;
}
.number .pay .pay-list .pay-top .top-right image {
  width: 35rpx;
  height: 35rpx;
  margin-left: 10rpx;
}
.number .pay .pay-list .pay-top .top-right text {
  font-size: 28rpx;
  color: #b6b6b6;
}
.number .pay .pay-list .pay-bot {
  width: 100%;
  height: 90rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.number .pay .pay-list .pay-bot .bot-left {
  width: 300rpx;
  height: 90rpx;
  display: flex;
  justify-content: flex-start;
  align-items: center;
}
.number .pay .pay-list .pay-bot .bot-left image {
  width: 45rpx;
  height: 45rpx;
}
.number .pay .pay-list .pay-bot .bot-left text {
  font-size: 28rpx;
  font-weight: bolder;
  color: #000;
  margin-left: 20rpx;
}
.number .pay .pay-list .pay-bot .bot-right {
  width: 300rpx;
  height: 90rpx;
  display: flex;
  justify-content: flex-end;
  align-items: center;
}
.number .pay .pay-list .pay-bot .bot-right image {
  width: 50rpx;
  height: 50rpx;
}
.fot {
  margin-top: 30rpx;
  width: 100%;
  text-align: center;
  font-size: 28rpx;
  color: #e5464a;
}
.footer {
  width: 710rpx;
  background: #fff;
  position: fixed;
  left: 0;
  bottom: 0;
  padding: 0 20rpx;
  height: 120rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.footer .money {
  width: 350rpx;
}
.footer .money .mone {
  font-size: 26rpx;
  color: #e5464a;
}
.footer .money .mtwo {
  margin-left: 30rpx;
  font-size: 26rpx;
  color: #e5464a;
}
.footer .money .mthree {
  font-size: 30rpx;
  font-weight: bolder;
  color: #e5464a;
}
.footer .payMoney {
  width: 270rpx;
  height: 90rpx;
  border-radius: 45rpx;
  text-align: center;
  line-height: 90rpx;
  background: #e5464a;
  color: #fff;
  font-size: 30rpx;
  font-weight: bolder;
}
