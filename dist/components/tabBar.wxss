.tabBar {
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 98rpx;
  background: #2d2d2d;
  display: flex;
  align-items: center;
  justify-content: center;
}
.tabBar view {
  width: 33%;
  text-align: center;
  font-size: 20rpx;
  color: #ffffff;
}
.tabBar view image {
  width: 40rpx;
  margin-top: 8rpx;
  height: 40rpx;
}
.tabBar view view {
  width: 100%;
  text-align: center;
  color: #4f4f4f;
}
.tabBar view .hover {
  color: #fff;
}
