.container {
  position: relative;
}
.my {
  min-height: 100%;
  background-color: #f2f2f2;
  position: relative;
}
.myHead {
  width: 100%;
  height: 161rpx;
  background: url("https://cdn.sunjoypai.com/images/4222/3853/0927/04e944fbb5a77d16d9b21ac197b30e1c_0_0.png") no-repeat center;
  background-size: 100% 100%;
}
.myIcon {
  width: 148rpx;
  height: 148rpx;
  border-radius: 50%;
  position: absolute;
  left: 300rpx;
  top: 58rpx;
}
.myName {
  width: 100%;
  height: 90rpx;
  line-height: 90rpx;
  padding-top: 50rpx;
  background: #fff;
  color: #000000;
  font-size: 32rpx;
  text-align: center;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.myId {
  width: 220rpx;
  height: 60rpx;
  font-size: 28rpx;
  color: #fff;
  position: absolute;
  right: 0rpx;
  top: 20rpx;
}
.myIcon image {
  width: 148rpx;
  height: 148rpx;
  border-radius: 50%;
}
.myfost {
  width: 100%;
  height: 160rpx;
  background: #fff;
  display: flex;
  justify-content: space-around;
  box-shadow: 1px 6px 3px #f2f2f2;
}
.myfost image {
  width: 70rpx;
  height: 70rpx;
  margin: 0 auto;
}
.myfost text {
  font-size: 30rpx;
}
.myfost div {
  text-align: center;
}
.mykf {
  width: 100%;
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  text-align: center;
}
.carnoBtn {
  width: 360rpx;
  height: 90rpx;
  background: #D0021B;
  border-radius: 7px;
  font-size: 33rpx;
  color: #fff;
  margin: 0 auto;
  text-align: center;
  line-height: 90rpx;
}
.order-box {
  width: 100%;
  height: auto;
  margin-top: 15rpx;
}
.order-title {
  width: 670rpx;
  padding: 0 40rpx;
  background: #fff;
  color: #000;
  font-size: 32rpx;
  height: 70rpx;
  line-height: 70rpx;
}
.order-con {
  width: 100%;
  height: auto;
  margin-top: 5rpx;
  padding-bottom: 110rpx;
}
.order-con-list {
  width: 670rpx;
  padding: 0 40rpx;
  height: 275rpx;
  margin-top: 5rpx;
  background: #fff;
}
.order-con-list-top {
  width: 100%;
  height: 70rpx;
  line-height: 70rpx;
  display: flex;
  justify-content: space-between;
}
.order-con-list-top-left {
  font-size: 28rpx;
  color: #9b9b9b;
}
.order-con-list-top-right {
  font-size: 28rpx;
  color: #fac03d;
}
.order-con-list-bottom {
  width: 100%;
  display: flex;
  justify-content: space-between;
}
.order-con-list-bottom-left {
  width: 160rpx;
  height: 180rpx;
  display: flex;
  align-items: center;
}
.order-con-list-bottom-left image {
  width: 160rpx;
  height: 170rpx;
}
.order-con-list-bottom-right {
  width: 480rpx;
  height: 170rpx;
}
.order-con-list-bottom-right-title {
  width: 100%;
  height: 40rpx;
  line-height: 40rpx;
  color: #000;
  font-size: 30rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.order-con-list-bottom-right-nub {
  line-height: 40rpx;
  color: #9b9b9b;
  font-size: 26rpx;
}
.order-con-list-bottom-right-zong {
  height: 40rpx;
  line-height: 40rpx;
  color: #d0021b;
  font-size: 26rpx;
}
.order-con-list-bottom-right-btn {
  width: 100%;
  height: 50rpx;
  margin-top: 10rpx;
  display: flex;
  justify-content: space-between;
}
.affirm,
.gobuy {
  width: 204rpx;
  height: 50rpx;
  border-radius: 4px;
  background: #4a90e2;
  text-align: center;
  line-height: 50rpx;
  color: #fff;
  font-size: 30rpx;
}
.mine-content {
  margin: 2rpx 0;
}
.tab-item {
  height: 90rpx;
  background: #fff;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
}
.tab-item-left {
  padding-left: 30rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
}
.tab-item-left text {
  color: #666;
  margin-left: 30rpx;
  font-size: 30rpx;
}
.arrow {
  width: 10rpx;
  height: 10rpx;
  border-top: 2rpx solid #dedede;
  border-right: 2rpx solid #dedede;
  transform: rotate(45deg);
}
