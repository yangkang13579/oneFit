@import "./../../components/headers.wxss";
page {
  background: #111111;
}
.times {
  height: 130rpx;
  background: #010101;
  width: 100%;
  padding: 35rpx 0;
}
.times .time {
  white-space: nowrap;
}
.times .time .block {
  color: #fff;
  font-size: 32rpx;
  width: 130rpx;
  text-align: center;
  margin: 5rpx 0;
  display: inline-block;
}
.times .time .block .date {
  width: 60rpx;
  border-radius: 50%;
  margin-left: 35rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  height: 50rpx;
  margin-bottom: 15rpx;
  color: #acacac;
}
.times .time .block .week {
  width: 60rpx;
  height: 60rpx;
  margin-left: 35rpx;
  text-align: center;
  line-height: 60rpx;
  font-size: 30rpx;
  border-radius: 50%;
  color: #acacac;
  border: 1rpx solid #acacac;
}
.times .time .block .hover {
  background: #e02510;
  color: #fff;
  border: 1rpx solid #e02510;
}
.times .time .block .color {
  color: #fff;
}
.times-point {
  margin-top: 30rpx;
  font-size: 34rpx;
  font-weight: bold;
  color: #e02510;
  margin-left: 20rpx;
}
.times-point .point-tit {
  display: flex;
  align-items: center;
}
.times-point image {
  width: 42rpx;
  height: 52rpx;
  margin-left: 20rpx;
}
.point {
  margin-top: 20rpx;
  width: 100%;
  height: 80rpx;
  white-space: nowrap;
  background: #2d2d2d;
}
.point .block {
  display: inline-block;
}
.point .block view {
  color: #acacac;
  width: 91rpx;
  text-align: center;
  height: 41rpx;
  margin-top: 19rpx;
  font-size: 26rpx;
  line-height: 41rpx;
  margin-right: 40rpx;
}
.point .block .hover {
  background: #e02510;
  color: #fff;
  border-radius: 30rpx;
}
.btn {
  margin-top: 69rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}
.btn view {
  width: 240rpx;
  height: 70rpx;
  border-radius: 35rpx;
  text-align: center;
  line-height: 70rpx;
  font-size: 32rpx;
}
.btn .btn-left {
  background: #e02510;
  color: #fff;
  margin-right: 75rpx;
}
.btn .btn-right {
  border: 1rpx solid #e02510;
  color: #fff;
}
.teacher {
  margin-top: 75rpx;
  position: relative;
  margin-left: 25rpx;
}
.teacher .icon8 {
  position: absolute;
  width: 702rpx;
  height: 450rpx;
  left: 0;
  top: 0;
}
.teacher .icon10 {
  position: absolute;
  width: 294rpx;
  height: 451rpx;
  left: 0;
  top: 0;
}
.teacher .teacher-name {
  position: absolute;
  right: 94rpx;
  top: 131rpx;
  width: 300rpx;
  height: 62rpx;
  font-size: 40rpx;
  color: #fff;
  line-height: 62rpx;
}
.teacher .teacher-name image {
  width: 300rpx;
  height: 62rpx;
  position: absolute;
}
.teacher .teacher-name text {
  margin-left: 10rpx;
  font-weight: bold;
  position: absolute;
  z-index: 2;
}
.teacher .introduce {
  width: 372rpx;
  position: absolute;
  right: 46rpx;
  top: 224rpx;
  line-height: 2;
  font-size: 24rpx;
  color: #fff;
}
.item-list {
  margin-top: 26rpx;
}
.item-list .item {
  margin: 0 26rpx;
  background: #2d2d2d;
  margin-bottom: 25rpx;
  height: 160rpx;
  display: flex;
  align-items: center;
  position: relative;
}
.item-list .item .logo {
  width: 140rpx;
  height: 140rpx;
  margin: 0 49rpx 0 33rpx;
}
.item-list .item .icon29 {
  width: 160rpx;
  height: 50rpx;
  position: absolute;
  right: 0;
  bottom: 33rpx;
}
.item-list .item .name .name-tips {
  font-size: 36rpx;
  font-weight: bold;
  color: #fff;
  margin-bottom: 31rpx;
}
.item-list .item .name .tips {
  color: #acacac;
  font-size: 24rpx;
}
.item-list .item .name .tips text {
  margin-right: 15rpx;
}
