@import "./../../components/headers.wxss";
page {
  background: #111111;
}
.data {
  width: 700rpx;
  padding: 35rpx 0;
  background: #2d2d2d;
  margin: 0 25rpx;
  border-radius: 10rpx;
}
.data .data-top {
  padding: 0 21rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  font-size: 36rpx;
  color: #e02510;
}
.data .data-top image {
  width: 158rpx;
  height: 33rpx;
}
.icon26 {
  width: 100%;
  height: 4rpx;
  margin-top: 39rpx;
}
.inputs {
  margin-top: 39rpx;
  padding: 0 21rpx;
  clear: both;
  overflow: hidden;
}
.inputs .inputs-left {
  float: left;
  font-size: 26rpx;
  color: #fff;
  display: flex;
  align-items: center;
  justify-content: center;
  margin-bottom: 20rpx;
}
.inputs .inputs-left text {
  width: 85rpx;
  text-align: left;
  margin-left: 20rpx;
}
.inputs .inputs-left input {
  width: 220rpx;
  height: 50rpx;
  border: 1rpx solid #acacac;
  border-radius: 10rpx;
}
.inputs .inputs-last {
  margin-bottom: 0;
}
.inputs .inputs-last text {
  width: 122rpx;
}
.inputs .inputs-last input {
  width: 504rpx;
}
.items {
  display: flex;
  align-items: center;
  justify-content: center;
  margin-top: 25rpx;
}
.items .item {
  margin-left: 20rpx;
  width: 210rpx;
  font-size: 30rpx;
  color: #fff;
  text-align: center;
  font-weight: bold;
}
.items .item image {
  width: 210rpx;
  height: 280rpx;
  margin-bottom: 16rpx;
}
