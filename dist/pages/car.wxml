
<view>
    <view class="bg"/>
      <view class="page-top">
        <view class="wrap">
        <view class="empty"  wx:if="{{list.length === 0}}">
          <view>暂无数据</view>
        </view>
        <block  wx:for="{{list}}" wx:key="{{index}}">
            <view class="list-wrap">
              <view class="middle">
                <view class="items">
                  <span>
                    {{item.meetingName}}
                  </span>
            
                </view>

                <view class="items">
                  <span>
                    签到时间：{{item.createDate}}
                  </span>
                 
                </view>
              </view>
            </view>
        </block>
        </view>
        </view>
  </view>
